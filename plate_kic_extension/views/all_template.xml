<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <template id="plate_kic_extension.lane_three_template">
            <t t-call="website.layout">
                <div id="lane_three_plate" class="main_body" />
            </t>
        </template>
        <template id="plate_kic_extension.lane_four_template">
            <t t-call="website.layout">
                <div id="lane_four_plate" class="main_body" />
            </t>
        </template>


        <!-- inherit loing template -->
        <template id='inherit_login' inherit_id="web.login">
            <xpath expr="//label[@for='login']" position="replace">
                <label for="login" class="form-label text-light">Email</label>
            </xpath>
            <xpath expr="//label[@for='password']" position="replace">
                <label for="password" class="form-label text-light">Password</label>
            </xpath>
           
        </template>
    </data>
</odoo>
